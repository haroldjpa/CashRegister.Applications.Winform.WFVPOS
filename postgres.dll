CREATE VIEW public.vwinvoice AS
 SELECT pa_invoice.total_value,
    pa_invoice.total_tax,
    pa_invoice.invoice_number,
    pa_invoice.print_number,
    pa_invoice.creation_date,
    ps_state.name AS state_name,
    ps_state."character",
    ad_user.nickname,
    pa_invoice_payment_type.name AS payment_type,
    pa_invoice_payment.value,
    pa_invoice_payment.approval,
    pa_invoice_payment.card_brand,
    pa_invoice_payment.receipt_number,
    pa_invoice_payment.aid,
    pa_invoice_payment.tvr,
    pa_invoice_payment.arqc,
    pa_invoice_payment.tc,
    pa_invoice_payment.tsi,
    pa_invoice_payment.na,
    pa_invoice_payment.terminal_id,
    pa_invoice_payment.acquire_id,
    pa_invoice_payment.pay_entry_mode,
    ps_item.name AS item_name,
    ps_measure_unit.abbreviation,
    pa_invoice_detail.unit_price,
    pa_invoice_detail.quantity,
    pa_invoice.invoice_id
   FROM public.pa_invoice,
    public.pa_invoice_detail,
    public.pa_invoice_payment,
    public.pa_invoice_payment_type,
    public.ps_state,
    public.ad_user,
    public.ps_item,
    public.ps_measure_unit
  WHERE ((pa_invoice.invoice_id = pa_invoice_payment.invoice_id) AND (pa_invoice.invoice_id = pa_invoice_detail.invoice_id) AND (pa_invoice_payment_type.invoice_payment_type_id = pa_invoice_payment.invoice_payment_type_id) AND (ps_state.state_id = pa_invoice.state_id) AND (ad_user.user_id = pa_invoice.user_id) AND (ps_item.item_id = pa_invoice_detail.item_id) AND (ps_measure_unit.measure_unit_id = ps_item.measure_unit_id))
  ORDER BY pa_invoice.invoice_id;


ALTER TABLE public.vwinvoice OWNER TO postgres;


CREATE VIEW public.vwitems AS
 SELECT ps_item.item_id,
    ps_item.name,
    ps_item.description,
    ps_item.bar_code,
    ps_item.unique_code,
    ps_item.min_value,
    ps_item.max_value,
    ps_item.image,
    ps_item.use_barcode,
    ps_item.use_unique_code,
    ps_item.use_expiration_date,
    ps_item.use_negative_balance,
    ps_item.use_tax,
    ps_item.price,
    ps_item.expiration_date,
    ps_item.creation_date,
    ps_item.last_timestamp,
    ps_state.name AS state_name,
    ps_state."character",
    ps_measure_unit.abbreviation,
    ps_measure_unit.factor,
    ps_item_category.name AS category_name,
    ps_company.name AS company_name,
    ps_item.state_id,
    ps_item.measure_unit_id,
    ps_item.item_category_id,
    ps_item.company_id
   FROM public.ps_item,
    public.ps_state,
    public.ps_measure_unit,
    public.ps_item_category,
    public.ps_company
  WHERE ((ps_item.state_id = ps_state.state_id) AND (ps_item.measure_unit_id = ps_measure_unit.measure_unit_id) AND (ps_item.company_id = ps_company.company_id) AND (ps_item.item_category_id = ps_item_category.item_category_id))
  ORDER BY ps_item.item_id;


ALTER TABLE public.vwitems OWNER TO postgres;


CREATE VIEW public.vwusers AS
 SELECT ad_user.user_id,
    ad_user.person_id,
    ad_user.nickname,
    ad_person.identification,
    ad_person.name,
    ad_person.last_name,
    ad_person.phone,
    ad_person.address,
    ad_person.email,
    ad_person.creation_date AS person_creation_date,
    ad_user.creation_date AS user_creation_date,
    ad_user.attemp_password,
    ps_role.name AS role_name,
    ps_role.role_id
   FROM (((public.ad_user
     JOIN public.ad_person ON ((ad_person.person_id = ad_user.person_id)))
     JOIN public.ps_person_user_role ON ((ad_user.user_id = ps_person_user_role.user_id)))
     JOIN public.ps_role ON ((ps_person_user_role.role_id = ps_role.role_id)))
  WHERE (ad_person.person_id = ad_user.person_id)
  ORDER BY ad_user.user_id;


ALTER TABLE public.vwusers OWNER TO postgres;


